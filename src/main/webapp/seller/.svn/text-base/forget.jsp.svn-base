<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="zh-CN">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
		<meta name="author" content="zhanglin" />
		<title>找回密码</title>
		<link rel="stylesheet" type="text/css" href="css/common.css">
		<link rel="stylesheet" type="text/css" href="css/login.css">
		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!--[if lt IE 9]>
		  <script src="//cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
		  <script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
		<script src="js/jquery-1.11.0.min.js"></script>
	    <script src="js/forget.js"></script>
	</head>
	<body>
		<div class="login_header">
			<div class="welcome">找回密码</div>
		</div>
		<div class="login_content" style="height:400px;">
			<div class="login_box">
				<div class="login_text">找回密码</div>
				<span style="display: block;height: 10px;color:red" id="check"></span>
				<form id="login_form" autocomplete="off" >
					<div class="group margin-bt">
						<label class="lb fl" for="userName">手机号</label>
						<div class="col-cms fl">
							<input name="userName"  type="text" class="mobile" placeholder="手机号" />
						</div>
					</div>
					<div class="group pdding_bj">
						<label class="lb fl" for="code">验证码</label>
						<div class="col-cms fl">
							<input autocomplete="off" class="code_w code" type="text" name="userName" placeholder="验证码" />
							<input type="button" id="getCheck" data="getCheck" class="getcode_w fr" style="cursor: pointer"  value="获取验证码" />
						</div>
					</div>
					<div class="group pdding_bj">
						<label class="lb fl" for="userName">密码</label>
						<div class="col-cms fl">
							<input type="password" name="clear_password" class="password" autocomplete="off" placeholder="密码" />
						</div>
					</div>
				</form>
				<div class="group" style="margin-bottom:10px;">
					<input type="button" class="dis" id="forgetBtn" style="cursor: pointer" value="提交" />
				</div>
			</div>
		</div>
	</body>
</html>