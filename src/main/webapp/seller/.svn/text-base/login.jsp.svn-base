	<body>
		<div class="login_header">
			<div class="welcome">欢迎登录</div>
		</div>
		<div class="login_content">
			<div class="login_box">
				<div class="login_text">登录</div>
				<!-- <form id="login_form"> -->
				<div style="height: 10px;"><label id="check" style="color: red"></label></div>
					<div class="group margin-bt">
						<label class="lb fl" for="userName">用户名</label>
						<div class="col-cms fl">
							<input autocomplete="off" autotype="text" class="userName" placeholder="用户名" />
							
						</div>
					</div>
					<div class="group pdding_bj">
						<label class="lb fl" for="userName">密码</label>
						<div class="col-cms fl">
							<input autocomplete="off" type="password" class="password" placeholder="密码" />
						</div>
					</div>
				<!-- </form> -->
				<div class="group" style="margin-top:5px;">
					<input type="button" class="dis" id="loginBtn" style="cursor: pointer" value="登录" />
				</div>
				<a class="forget_pwd" href="forget.jsp">忘记密码？</a>
			</div>
		</div>
