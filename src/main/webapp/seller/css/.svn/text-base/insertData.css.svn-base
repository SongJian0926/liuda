.men{
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0px;
    top: 0px;
    background: rgba(0,0,0,0.5);
    z-index: 999;
}
.ban{
    position: fixed;
    width: 320px;
    height: 200px;
    left: 60%;
    top: 50%;
    margin:-150px auto auto -300px;
    z-index: 1000;
    background-color:white;
    border-radius: 4px;
}
.ban .ban_p1{
    margin-top: 35px;
    text-align: center;
    font-size: 20px;
    color: #00b7b2;
}
.ban .ban_p2{
   padding: 5px;
   text-align: center;
}
.ban .ban_button{
    margin-top: 8px;
    margin-left: 55px;
    height: 35px;
    width: 200px;
    background-color: #00b7b2;
    color: #fff;
    border: none;
    border-radius: 4px;
}
.insert-context{
    padding-top: 45px;
    padding-button: 45px;
    padding-left: 135px;
    padding-right: 55px;
    background-color: #fff;
}
.select{
    overflow: hidden;
}
.select label{
    float: left;
    margin-right: 56px;
}
.context{
    padding-top: 15px;
}
.context label{
    margin-right: 88px;
}
.f1 label{
    margin-right: 60px;
}
.f3 label{
    margin-right: 74px;
}
.context input{
	padding: 0 3px;
}
.context input{
    width: 314px;
    border: 1px solid #d8d8d8;
    height: 30px;
}
.context textarea{
	width: 316px;
    border: 1px solid #d8d8d8;
    height: 30px;
}
.context textarea{
    height: 70px;
    resize: none;
}
.f3 input{
    width: 222px;
}
.f3 button{
    width: 90px;
    height: 30px;
    background-color: #00b7b2;
    color: #fff;
    border: hidden;
}
.submit{
    margin-left: 110px;
    margin-top: 48px;
}
.submit input{
    width: 240px;
    height: 38px;
    background-color: #00b7b2;
    color: #FFF;
    border: none;
    border-radius: 4px;
}
.upload_div{
	width: 73px;
    overflow: hidden;
    height: 73px;
    border: none;
    border-radius: 4px;
    text-align: center;
    position: relative;
    line-height: 100px;
}
.upload_file{
	position: absolute;
    top: 0;
    right: 0;
    opacity: 0;
    height: 100%;
    cursor: pointer;
    width: 73px;
}
.li-img{
	float: left;
	top: 0px;
}
.add_img{
	overflow: hidden;
	margin-top: 14px;
}
.memo{
	margin-left: 110px;
}
.memo span{
	color: #00b7b2;
}
.edit{
	margin-left: 120px;
	color: #00b7b2;
}

a{
	color: #00b7b2;
}
.font-sty{
	display: block;
	width: 80px;
	margin-right: 36px;
}
.checklogo{
	width: 20px;
	margin: 0px;
}
.warn{
	color:#FF0000;
	font-size:15px;
}