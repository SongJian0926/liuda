.men{
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0px;
    top: 0px;
    background: rgba(0,0,0,0.5);
    z-index: 999;
}
.ban{
    position: fixed;
    width: 320px;
    height: 200px;
    left: 60%;
    top: 50%;
    margin:-150px auto auto -300px;
    z-index: 1000;
    background-color:white;
    border-radius: 4px;
}
.ban .ban_p1{
    margin-top: 35px;
    text-align: center;
    font-size: 15px;
    color: #00b7b2;
}
.ban .ban_p2{
   padding: 5px;
   text-align: center;
   color: #00b7b2;
}
.ban .ban_button{
    margin-top: 8px;
    margin-left: 55px;
    height: 35px;
    width: 200px;
    background-color: #00b7b2;
    color: #fff;
    border: none;
    border-radius: 4px;
}
.right_content{
	background-color:#fff;
}
.box-font{
    width: 100%;
    height: 155px;
    line-height: 130px;
    background: url('../images/cash.png') no-repeat 340px 5px;
}
.box-font > p{
    width:290px;
    height:10px;
	margin-left:270px;
    font-size: 20px;
    color: #00B6B2;
}
.line{
    width:750px;
    height:1px;
    overflow:hidden;
    border-top:1px solid #d8d8d8;
}
.box-body{
    padding-top: 80px;
    padding-left:185px;
    padding-right:185px;
}
.box-body label{
    float: left;
    margin-right: 20px;
    margin-top: 5px;
    color: #0f0f0f;
}
.pwd label{
    margin-right: 18px;
}
.password{
    padding-bottom: 14px;
}
form input{
    width: 300px;
    border: 1px solid #d8d8d8;
}
.test input{
    width: 195px;
    margin-right: 10px;
}
.test-pwd{
    height: 33px;
    background-color:#00B6B2;
    color: white;
    border: none;
    padding: 0 12px;
}
.box-body p{
    margin-left: 80px;
    color: #d4d4d4;
}
.account{
    width: 395px;
}
.group {
   padding: 70px 80px 70px 70px;
}
.box-body form{
    width: 395px;
}
.group input{
    width: 230px;
    height: 30px;
    background-color:#00B6B2;
    border-radius: 6px;
    color: white;
    border: none;
}
.innerspan{
	font-size: 14px;
}
.warn{
	color:#FF0000;
	font-size:15px;
}